<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>downExcel</servlet-name>
        <servlet-class>downExcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadExcel</servlet-name>
        <servlet-class>uploadExcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveToDB</servlet-name>
        <servlet-class>saveToDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>downTemplate</servlet-name>
        <servlet-class>downTemplate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>downExcel</servlet-name>
        <url-pattern>/downExcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadExcel</servlet-name>
        <url-pattern>/uploadExcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveToDB</servlet-name>
        <url-pattern>/saveToDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>downTemplate</servlet-name>
        <url-pattern>/downTemplate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
